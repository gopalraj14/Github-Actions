name: Gohell
on: push

jobs:
  hell:
    runs-on: ubuntu-latest

    steps:
      - name: Checkout repository
        uses: actions/checkout@v2

      - name: Create directory
        run: mkdir -p https://github.com/gopalraj14/Github-Actions/blob/main

      - name: Verify directory creation
        run: ls -la https://github.com/gopalraj14/Github-Actions/blob/main
